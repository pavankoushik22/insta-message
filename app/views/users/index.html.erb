<h1 align="center"> All Users</h1>
<div align="center">
<%= will_paginate %>
<% @users.each do |user| %>
<% if user != current_user %>
<ul class="listing">
<div class="row">
<div class="well col-md-4 col-md-offset-4">

<li class="article-title"><a href="<%= user_path(user) %>/#message"><%= user.email %></a></li>


</div>
</div>
</ul>
<% end %>

	
<%end%>
<%= will_paginate %>
</div>